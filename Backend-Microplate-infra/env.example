# Infrastructure - used by docker-compose.apps.yml only
# Copy to .env before running: docker compose -f docker-compose.apps.yml up

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=microplate

# RabbitMQ
RABBITMQ_USER=microplate
RABBITMQ_PASS=microplate123

# Rate limiting (optional; defaults in compose: 60000, 10000)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=10000
RATE_LIMIT_MAX_REQUESTS=10000
