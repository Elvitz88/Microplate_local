{
  "name": "prediction-db-service",
  "version": "1.0.0",
  "description": "Database service for managing prediction data with Prisma",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "tsx src/seed.ts",
    "db:reset": "prisma migrate reset --force --skip-seed",
    "db:seed": "npm run prisma:seed",
    "db:studio": "prisma studio",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "express",
    "prisma",
    "typescript",
    "microservice",
    "prediction",
    "database"
  ],
  "author": "Microplate AI Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "winston": "^3.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "globals": "^16.5.0",
    "knip": "^5.24.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.47.0"
  }
}
